{
    "tabWidth": 4,
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true,
    "semi": true,
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrder": [
        "^react",
        "<THIRD_PARTY_MODULES>",
        "^pages/(.*)$",
        "^@component/(.*)$",
        "^@ui/(.*)$",
        "^@shared/(.*)$",
        "^@assets/(.*)$",
        "^@utils/(.*)$",
        "^@store/(.*)$",
        "^../(.*)",
        "^./(.*)",
        "(.scss)$"
    ]
}
